                  "Minnesota"
                ],
                [
                  "Minneapolis"
                ]
              ],
              "returns": {
                "county": "Hennepin County",
                "name": "Minneapolis",
                "state": "Minnesota",
                "coordinates": [
                  "44.9871, -93.2737"
                ],
                "country": "United States"
              }
            },
            {
              "matches": [
                [
                  "Alexandria",
                  "Douglas",
                  "Minnesota",
                  "United States",
