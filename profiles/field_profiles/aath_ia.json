      },
      {
        "process": "gsub",
        "args": {
          "pattern": "vlogs",
          "replacement": "vlogs"
        }
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "youth_media",
          "replacement": "Youth Media"
        }
      },
      {
        "process": "gsub",
        "args": {
