  "sourceResource/type": {
    "origins": [
      {
        "path": "mediatype"
      }
    ],
    "processors": [
      {
        "process": "gsub",
        "args": {
          "pattern": "movies",
          "replacement": "video"
        }
      }
    ]
  },
  "identifier": {
    "origins": [
      {
        "path": "identifier"
      }
    ]
  },
  "matcher": {
    "origins": [
      {
        "path": "collection"
      },
      {
        "path": "title"
      },
      {
        "path": "subject"
      },
      {
        "path": "description"
      }
    ],
    "processors": [
      {
        "process": "flatten"
      },
      {
        "process": "join",
        "args": [
          " "
        ]
      },
      {
        "process": "downcase"
      },
      {
        "process": "match_first",
        "args": {
          "enrichment": "aath_terms"
        }
      }
    ],
    "record_filters": [
      {
        "run": "after_processors",
        "reject_unless": "has_value?"
      }
    ]
  },
  "matcherOriginal": {
    "origins": [
      {
        "path": "collection"
      },
      {
        "path": "title"
      },
      {
        "path": "subject"
      },
      {
        "path": "description"
      }
    ],
    "processors": [
      {
        "process": "flatten"
      },
      {
        "process": "join",
        "args": [
          "  "
        ]
      },
      {
        "process": "downcase"
      }
    ]
  }
}
