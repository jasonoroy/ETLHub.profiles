{
  "sourceResource/language": {
    "origins": [
      {
        "path": "title"
      },
      {
        "path": "description"
      }
    ],
    "processors": [
      {
        "process": "join",
        "args": [
          " "
        ]
      },
      {
        "process": "text_to_language_code"
      },
      {
        "process": "match_first",
        "args": {
          "pattern": "en"
        }
      }
    ],
    "record_filters": [
      {
        "run": "after_processors",
        "reject_unless": "has_value?"
      }
    ]
  },
  "sourceResource/rights": {
    "origins": [
      {
        "path": "licenseurl"
      }
    ]
  },
  "object": {
    "path_keys": [
      {
        "token": "$identifier",
        "path": "identifier"
      }
    ],
    "origins": [
      {
        "url": "https://archive.org/details/$identifier"
      }
    ],
    "processors": [
      {
        "process": "attribute_value_at_path",
        "args": {
          "path": "//meta[@property='og:image']",
          "attribute": "content"
        }
      }
    ]
  },
  "isShownAt": {
    "origins": [
      {
        "path": "identifier"
      }
    ],
    "processors": [
      {
        "process": "affix",
        "args": {
          "prefix": "https://archive.org/details/"
        }
      }
    ]
  },
  "record_hash": {
    "origins": [
      {
        "path": "identifier"
      }
    ],
    "processors": [
      {
        "process": "affix",
        "args": {
          "prefix": "https://archive.org/details/"
        }
      },
      {
        "process": "to_sha1_hex"
      }
    ]
  },
  "sourceResource/title": {
    "origins": [
      {
        "path": "title"
      }
    ],
    "processors": [
      {
        "process": "strip"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\s+\\s+",
          "replacement": " "
        }
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\;$",
          "replacement": ""
        }
      }
    ]
  },
  "sourceResource/subject": {
    "origins": [
      {
        "path": "subject"
      }
    ],
    "processors": [
      {
        "process": "split",
        "args": [
          ", "
        ]
      },
      {
        "process": "flatten"
      },
      {
        "process": "unique"
      },
      {
        "process": "strip"
      },
      {
        "process": "sort"
      },
      {
        "process": "apply_label",
        "args": "name"
      }
    ]
  },
  "sourceResource/description": {
    "origins": [
      {
        "path": "description"
      }
    ]
  },
  "sourceResource/date/begin": {
    "origins": [
      {
        "path": "publicdate"
      }
    ],
    "processors": [
      {
        "process": "to_year"
      }
    ]
  },
  "sourceResource/date/end": {
    "origins": [
      {
        "path": "publicdate"
      }
    ],
    "processors": [
      {
        "process": "to_year"
      }
    ]
  },
  "sourceResource/date/displayDate": {
    "origins": [
      {
        "path": "publicdate"
      }
    ]
  },
  "sourceResource/format": {
    "origins": [
      {
        "path": "form"
      }
    ],
    "processors": [
      {
        "process": "unique"
      },
      {
        "process": "strip"
      },
      {
        "process": "sort"
      }
    ]
  },
  "sourceResource/collection/title": {
    "origins": [
      {
        "path": "collection"
      }
    ],
    "processors": [
