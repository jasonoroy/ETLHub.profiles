      },
      {
        "process": "gsub",
        "args": {
          "pattern": "audio_music",
          "replacement": "Music, Arts & Culture"
        }
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "netlabels",
          "replacement": "Netlabels"
        }
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "audio_news",
