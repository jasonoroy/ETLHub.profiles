{
  "status": {
    "origins": [
      {
        "path": "header/status"
      }
    ]
  },
  "sourceResource/language": {
    "origins": [
      {
        "path": "metadata/dc/language"
      }
    ],
    "processors": [
      {
        "process": "encode_language_by_name"
      }
    ]
  },
  "isShownAt": {
    "origins": [
      {
        "path": "metadata/dc/identifier[last()]||metadata/dc/identifier"
      }
    ]
  },
  "sourceResource/extent": {
    "origins": [
      {
        "path": "metadata/dc/format[first()]||metadata/dc/format"
      }
    ]
  },
  "record_hash": {
    "origins": [
      {
        "path": "header/identifier"
      }
    ],
    "processors": [
      {
        "process": "join",
        "args": [
          ""
        ]
      },
      {
        "process": "to_sha1_hex"
      }
    ]
  },
  "dataProvider": {
    "origins": [
      {
        "path": "metadata/dc/publisher"
      }
    ]
  },
  "provider": {
    "origins": [
      {
        "value": {
          "@id": "http://dp.la/api/contributor/umn",
          "name": "University of Minnesota Libraries"
        }
      }
    ]
  },
  "identifier": {
    "origins": [
      {
        "path": "header/identifier"
      }
    ]
  },
  "sourceResource/title": {
    "origins": [
      {
        "path": "metadata/dc/title"
      }
    ],
    "processors": [
      {
        "process": "strip"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\s+\\s+",
          "replacement": " "
        }
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\;$",
          "replacement": ""
        }
      }
    ]
  },
  "sourceResource/rights": {
    "origins": [
      {
        "path": "metadata/dc/rights"
      }
    ],
    "processors": [
      {
        "process": "strip"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\s+\\s+",
          "replacement": " "
        }
      }
    ]
  },
  "sourceResource/spatial": {
    "origins": [
      {
        "path": "metadata/dc/coverage"
      }
    ],
    "processors": [
      {
        "process": "gsub",
        "args": {
          "pattern": "unknown",
          "replacement": ""
        }
      },
      {
        "process": "geonames_postal",
        "args": {
          "enrichment": "geonames_minnesota_defaults"
        }
      }
    ]
  },
  "sourceResource/subject": {
    "origins": [
      {
        "path": "metadata/dc/subject"
      }
    ],
    "processors": [
      {
        "process": "flatten"
      },
      {
        "process": "split",
        "args": [
          "; "
        ]
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\s--\\s",
          "replacement": "--"
        }
      },
      {
        "process": "split",
        "args": [
          "--"
        ]
      },
      {
        "process": "unique"
      },
      {
        "process": "sort"
      },
      {
        "process": "strip"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\"",
          "replacement": ""
        }
      },
      {
        "process": "flatten"
      },
      {
        "process": "apply_label",
        "args": "name"
      }
    ]
  },
  "sourceResource/date/begin": {
    "origins": [
      {
        "path": "metadata/dc/date[first()]||metadata/dc/date"
      }
    ],
    "processors": [
      {
        "process": "gsub",
        "args": {
          "pattern": "\\?",
          "replacement": ""
        }
      },
      {
        "process": "split",
        "args": [
          " - "
        ]
      },
      {
        "process": "first"
      },
      {
        "process": "split",
        "args": [
          "-"
        ]
      },
      {
        "process": "first"
      }
    ]
  },
  "sourceResource/date/end": {
    "origins": [
      {
        "path": "metadata/dc/date[last()]||metadata/dc/date"
      }
    ],
    "processors": [
      {
        "process": "gsub",
        "args": {
          "pattern": "\\?",
          "replacement": ""
        }
      },
      {
        "process": "split",
        "args": [
          " - "
        ]
      },
      {
        "process": "last"
      },
      {
        "process": "split",
        "args": [
          "-"
        ]
      },
      {
        "process": "first"
      }
    ]
  },
  "sourceResource/date/displayDate": {
    "origins": [
      {
        "path": "metadata/dc/date[first()]||metadata/dc/date"
      }
    ],
    "processors": [
      {
        "process": "join",
        "args": [
          " - "
        ]
      }
    ]
  },
  "sourceResource/creator": {
    "origins": [
      {
        "path": "metadata/dc/creator"
      }
    ],
    "processors": [
      {
        "process": "gsub",
        "args": {
          "pattern": "\\.$",
          "replacement": ""
        }
      }
    ]
  },
  "sourceResource/description": {
    "origins": [
      {
        "path": "metadata/dc/description"
      }
    ],
    "processors": [
      {
        "process": "join",
        "args": [
          " "
        ]
      },
      {
        "process": "strip"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "\\s+\\s+",
          "replacement": " "
        }
      }
    ]
  },
  "sourceResource/contributor": {
    "origins": [
      {
        "path": "metadata/dc/contributor"
      }
    ]
  },
  "sourceResource/format": {
    "origins": [
      {
        "path": "metadata/dc/format[last()]||metadata/dc/format"
      }
    ],
    "processors": [
      {
        "process": "strip"
      },
      {
        "process": "flatten"
      },
      {
        "process": "unique"
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "[[:punct:]]",
          "replacement": ""
        }
      }
    ]
  },
  "sourceResource/relation": {
    "origins": [
      {
        "path": "metadata/dc/relation"
      }
    ],
    "processors": [
      {
        "process": "split",
        "args": [
          ";"
        ]
      },
      {
        "process": "strip"
      }
    ]
  },
  "sourceResource/collection/title": {
    "path_keys": [
      {
        "token": "$setSpec",
        "path": "header/setSpec"
      }
    ],
    "origins": [
      {
        "path": "enrichments/listSets/records/$setSpec/setDescription/oclcdc/title"
      }
    ]
  },
  "sourceResource/collection/description": {
    "path_keys": [
      {
        "token": "$setSpec",
        "path": "header/setSpec"
      }
    ],
    "origins": [
      {
        "path": "enrichments/listSets/records/$setSpec/setDescription/oclcdc/description"
      }
    ]
  },
  "sourceResource/type": {
    "origins": [
      {
        "path": "metadata/dc/type"
      }
    ],
    "processors": [
      {
        "process": "flatten"
      },
      {
        "process": "unique"
      },
      {
        "process": "sort"
      },
      {
        "process": "downcase"
      },
      {
        "process": "join",
        "args": " "
      },
      {
        "process": "gsub",
        "args": {
          "pattern": "[[:punct:]]",
          "replacement": ""
        }
      },
      {
        "process": "gsub",
        "args": {
          "enrichment": "dc_text_type_replacements"
        }
      },
      {
        "process": "gsub",
        "args": {
          "enrichment": "dc_image_type_replacements"
        }
      },
      {
        "process": "gsub",
        "args": {
          "enrichment": "dc_sound_type_replacements"
        }
      },
      {
        "process": "gsub",
        "args": {
          "enrichment": "dc_video_type_replacements"
        }
      }
    ]
  }
}
